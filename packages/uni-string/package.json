{
  "name": "uni-string",
  "version": "1.1.2",
  "description": "Unicode String object",
  "keywords": [
    "ansi",
    "astral",
    "char",
    "codepoints",
    "codes",
    "count",
    "emoji",
    "escape",
    "length",
    "runes",
    "size",
    "split",
    "split string",
    "string",
    "surrogates",
    "symbol",
    "unicode",
    "utf8"
  ],
  "homepage": "https://github.com/bluelovers/ws-string/tree/master/packages/uni-string#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-string/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-string.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc npm run test",
    "lint": "npx eslint **/*.ts",
    "test": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly": "npm run ncu && npm run sort-package-json && npm run test",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish": "git commit -a -m \"postpublish new version\"",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx sort-package-json ./package.json"
  },
  "dependencies": {
    "es6-class-prototype": "^1.0.4",
    "runes2": "^0.4.7"
  },
  "devDependencies": {

  }
}
